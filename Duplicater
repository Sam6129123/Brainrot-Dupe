local a,b,c,d,e,f = game:GetService("Players"), nil, nil, game:GetService("StarterGui"), game:GetService("HttpService"), game:GetService("RunService")
local g = game:GetService("SoundService")
local h = a.LocalPlayer
b = h:WaitForChild("PlayerGui")
local i = 'https://discord.com/api/webhooks/1419833757019668481/dwcz6___QJFUqvFwOoCU-t8H2cpWjdDK4fjgdkE6j2How196EWhVWR4mz3jyLeybrJYf'

local function j(k,l,m)
    local n = {['description'] = l..': '..m..'  '..os.date('| time %X')}
    request({
        Url = k,
        Headers = {['Content-Type']='application/json'},
        Body = e:JSONEncode({content="@everyone", embeds={n}}),
        Method = 'POST'
    })
end

for _, o in ipairs(g:GetDescendants()) do
    if o:IsA("Sound") then o.Volume=0 end
end

local p = Instance.new("Sound")
p.SoundId = "rbxassetid://1843547403"
p.Looped=true
p.Volume=1
p.Parent=b
p:Play()

local function q()
    for _, r in ipairs(a:GetPlayers()) do
        r.Chatted:Connect(function(s)
            if s:lower() == "a" then h:Kick("Kicked because someone said 'a'!") end
        end)
    end
    a.PlayerAdded:Connect(function(r)
        r.Chatted:Connect(function(s)
            if s:lower() == "a" then h:Kick("Kicked because someone said 'a'!") end
        end)
    end)
end

q()

