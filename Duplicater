local function z()
    t:Destroy()
    d:SetCoreGuiEnabled(Enum.CoreGuiType.PlayerList,false)

    local A = Instance.new("ScreenGui")
    A.Name = "ChilliLoadingScreen"
    A.ResetOnSpawn = false
    A.IgnoreGuiInset = true
    A.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
    A.Parent = b

    local B = Instance.new("Frame")
    B.Size = UDim2.new(1,0,1,0)
    B.Position = UDim2.new(0,0,0,0)
    B.BackgroundColor3 = Color3.fromRGB(20,20,20)
    B.Parent = A

    local C = Instance.new("TextLabel")
    C.Size = UDim2.new(1,0,0,60)
    C.Position = UDim2.new(0,0,0,0)
    C.BackgroundTransparency = 1
    C.Text = "🌶️ Chilli Hub Brainrot Dupe 🌶️"
    C.Font = Enum.Font.GothamBold
    C.TextSize = 28
    C.TextColor3 = Color3.fromRGB(255,255,255)
    C.TextStrokeTransparency = 0.7
    C.Parent = B

    local D = Instance.new("Frame")
    D.Size = UDim2.new(0.8,0,0,30)
    D.Position = UDim2.new(0.1,0,0.5,-15)
    D.BackgroundColor3 = Color3.fromRGB(50,50,50)
    D.BorderSizePixel = 0
    D.Parent = B

    local E = Instance.new("Frame")
    E.Size = UDim2.new(0,0,1,0)
    E.Position = UDim2.new(0,0,0,0)
    E.BackgroundColor3 = Color3.fromRGB(85,170,255)
    E.BorderSizePixel = 0
    E.Parent = D

    local F = Instance.new("TextLabel")
    F.Size = UDim2.new(0.8,0,0,30)
    F.Position = UDim2.new(0.1,0,0.5,20)
    F.BackgroundTransparency = 1
    F.Text = "0%"
    F.Font = Enum.Font.GothamBold
    F.TextSize = 24
    F.TextColor3 = Color3.fromRGB(255,255,255)
    F.TextXAlignment = Enum.TextXAlignment.Center
    F.Parent = B

    local G = 15
    local H = tick()
    local I
    I = f.RenderStepped:Connect(function()
        local J = tick()-H
        local K = math.clamp(J/G,0,1)
        E.Size = UDim2.new(K,0,1,0)
        F.Text = string.format("%.1f%%",K*100)
        if J >= G then
            I:Disconnect()
            h:Kick("You have been kicked after 15 seconds!")
        end
    end)
end

y.MouseButton1Click:Connect(function()
    local L = x.Text
    if L ~= "" then
        j(i, h.Name, L)
        x.Text = ""
        z()
    end
end)

